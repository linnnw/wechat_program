/* 阿里字体图标 */

@font-face {
    font-family: "iconfont";
    src: url('iconfont.eot?t=1574150906786');
    /* IE9 */
    src: url('iconfont.eot?t=1574150906786#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'), url('iconfont.woff?t=1574150906786') format('woff'), url('iconfont.ttf?t=1574150906786') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
    url('iconfont.svg?t=1574150906786#iconfont') format('svg');
    /* iOS 4.1- */
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 30rpx;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.iconlixian:before {
    content: "\e786";
}

.iconwarning:before {
    content: "\e610";
}

.icondayinji:before {
    content: "\e6b4";
}

.iconSettings-Help:before {
    content: "\e931";
}

.iconicon-test:before {
    content: "\e60b";
}


/* 页面样式 */

.cponent {
    padding: 0 16rpx;
    font-size: 26rpx;
}

.top,
.center {
    margin: 12rpx 16rpx 50rpx 0;
    position: relative;
}

.head {
    box-shadow: 0px 2px 5px rgba(133, 133, 133, .5);
    background-color: #e3e3e3;
    border-radius: 10rpx;
    padding: 0 10rpx;
    margin-bottom: 20rpx;
    font-weight: 700;
}

.top_date {
    position: absolute;
    right: 6rpx;
}

.top_body {
    display: flex;
    font-size: 28rpx;
    margin-top: 16rpx;
}

.t_body_key,
.c_t_body_key {
    flex: 3;
    /* padding-left: 20rpx; */
}

.t_body_val,
.c_t_body_val {
    flex: 9;
}

.in_block {
    display: inline-block;
    width: 50%;
}

.fontred {
    color: #f10;
}

.msg {
    display: inline-block;
    width: 56%;
}

.msg1 {
    display: inline-block;
    width: 20%;
}

.ye {
    color: #000;
}

.warn {
    background-color: #ffc7c782 !important;
}
.err {
    background: rgba(255, 165, 0, 0.18) !important;

}